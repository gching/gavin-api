    <div class="valign-wrapper content-overlay" style="height: 100%;">
      <div class="valign center-align container" style="">
        <div class="z-depth-5 container" style="padding: 30px;">
          <div class="circle-header">
            <img src="<%= cloudinary.url('gavin_1_yheigu.png', trans.common_lossy) %>" alt="" class="circle responsive-img">
          </div>
          <h1 class="header white-text">Gavin Ching</h1>
